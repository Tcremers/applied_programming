void loadFile(){
	ifstream inFile("week7_8\\src\\dataX.dat");
//	assert(inFile.good());
	string line;

	while (getline(inFile, line)) {
		rowvec x(line);
		x.print();
		cout << line << endl;
	    // output the line
	    // now we loop back and get the next line in 'str'
	}
	cout << "Done" << endl;
}









