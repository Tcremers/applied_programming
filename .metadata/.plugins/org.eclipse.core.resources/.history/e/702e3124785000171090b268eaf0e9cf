#include <fstream>
#include <iostream>
#include <string>
#include <cassert>
#include <vector>
#include "NearestNeighbours.h"
#include "GetData.h"
#include "../armadillo.hpp"


using namespace std;
using namespace arma;







void kNearestNeighbours(){
	GetData data;
	int k = 5;
	for(int i = 0; i < data.getXTestData().size(); i++){
		vector<pair<double,int>> scorePairs;
		for(int j = 0; j < data.getXData().size(); j++){
			pair<double,int> foo;
			foo = make_pair(norm(data.getXTestData()[i] - data.getXData()[j]), data.getYdata()[j]);
			scorePairs.push_back(make_pair( ));
		}
		sort(scorePairs);
	}
}


